{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ]
    },
    "checkout_session": {
      "type": "string"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "total": {
      "type": "decimal"
    },
    "email": {
      "type": "string",
      "required": false
    },
    "downloads": {
      "type": "integer",
      "default": 0
    },
    "isBlocked": {
      "type": "boolean",
      "default": false
    },
    "promo_code": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::promo-code.promo-code"
    }
  }
}
